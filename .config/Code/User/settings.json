{
    "editor.fontFamily": "Iosevka",
    "editor.minimap.enabled": true,
    "editor.detectIndentation": true,
    "files.associations": {
        "env-example": "shellscript",
        "*.pl": "prolog"
    },
    "workbench.startupEditor": "welcomePage",
    "files.trimTrailingWhitespace": true,
    "editor.cursorSurroundingLines": 8,
    "php.suggest.basic": false,
    "breadcrumbs.enabled": true,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "[jade]": {},
    "editor.lineHeight": 24,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/bin/nvim",
    "vim.easymotion": true,
    "vim.sneak": true,
    "vim.handleKeys": {
        "<C-w>": false,
        "<C-p>": false,
        "<C-q>": false
    },
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "#a9dc7660",
    "vim.highlightedyank.duration": 250,
    "vim.normalModeKeyBindings": [
        {
        "before": ["g", "p", "d"],
        "commands": ["editor.action.peekDefinition"]
        },
        {
        "before": ["g", "h"],
        "commands": ["editor.action.showDefinitionPreviewHover"]
        },
        {
        "before": ["g", "i"],
        "commands": ["editor.action.goToImplementation"]
        },
        {
        "before": ["g", "p", "i"],
        "commands": ["editor.action.peekImplementation"]
        },
        {
        "before": ["g", "q"],
        "commands": ["editor.action.quickFix"]
        },
        {
        "before": ["g", "r"],
        "commands": ["editor.action.referenceSearch.trigger"]
        },
        {
        "before": ["g", "t"],
        "commands": ["editor.action.goToTypeDefinition"]
        },
        {
        "before": ["g", "p", "t"],
        "commands": ["editor.action.peekTypeDefinition"]
        },
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.synctex.synctexjs.enabled": false,
    "editor.rulers": [
        80
    ],
    "git.enableSmartCommit": true,
    "editor.fontLigatures": true,
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "--shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "editor.fontSize": 13,
    "markdown-preview-enhanced.automaticallyShowPreviewOfMarkdownBeingEdited": true,
    "markdown-preview-enhanced.mathRenderingOption": "None",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb",
        "*.docx": "default"
    },
    "mesonbuild.configureOnOpen": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.colorTheme": "Ocean Dark Extended",
    "haskell.serverExecutablePath": "/usr/bin/haskell-language-server-8.8.4",
    "shellcheck.enable": false,
    "rewrap.autoWrap.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "cSpell.enableFiletypes": [
        "!haskell"
    ],
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "window.zoomLevel": 1,
    "[latex]": {
        "editor.defaultFormatter": "mathematic.vscode-latex"
    },
    "latex-workshop.synctex.afterBuild.enabled": true,
    "cSpell.userWords": [
        "alloc",
        "curr",
        "cvar",
        "endignore",
        "insts",
        "LPVM",
        "optimisation",
        "optimizable",
        "struct",
        "TCMC",
        "Wybe"
    ]
}
