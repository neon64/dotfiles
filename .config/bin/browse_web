#!/usr/bin/env fish

switch $argv
case "http://*"
    set url $argv
case "https://*"
    set url $argv
case "*"
    echo "Searching google for '$argv'"
    set url "https://www.google.com/search?q=$argv"
end
systemd-cat xdg-open $url &
disown

## try one or the other

if type -q swaymsg
    swaymsg workspace 1
end

# else if type -q i3-msg
#     i3-msg workspace 1 >/dev/null 2>&1
# end
